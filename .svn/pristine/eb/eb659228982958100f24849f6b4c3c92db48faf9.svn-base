<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:tx="http://www.springframework.org/schema/tx"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
	
	<bean id="authenticateDao" class="com.inveno.cps.authenticate.dao.AuthenticateDao">
		<property name="sessionFactory">
			<ref bean="sessionFactory" />
		</property>
	</bean>
	
	<bean id="authenticateService.Iface" class="com.inveno.cps.authenticate.service.AuthenticateServiceImpl">
		<property name="authenticateDao">
			<ref bean="authenticateDao" />
		</property>
	</bean>
	
</beans>
